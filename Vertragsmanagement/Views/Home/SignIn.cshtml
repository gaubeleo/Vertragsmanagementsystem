
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Anmeldung - Vertragsmanagement</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
        <div class="col-md-12">
            <div class="panel panel-default">
                <div class="col-md-offset-4">
                    <div class="panel-body">
                        <div class="img-responsive">
                            <img src="~/Content/Images/Bezirkskliniken_Schwaben_Logo.svg.png" div class="col-md-6">
                        </div>
                    </div>
                </div>
            </div>
        </div>

    <div class="container-fluid body-content">
        <div class="jumbotron text-center">
            <h1>Vertragsmanagement</h1>
            @*<p class="lead">Applikation zur Verwaltung von unternehmensspezifischen Verträgen und der Überwachung der Fristen und Termine</p>*@
            @Html.ActionLink("Anmelden", "SignIn", "Account", routeValues: null, htmlAttributes: new { id = "loginLink", @class = "btn btn-primary" })
        </div>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
</body>
</html>




